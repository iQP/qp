<!DOCTYPE html>
<html>
<head>
	<title>{% block title %}Back End{% endblock %}</title>

	<meta charset="UTF-8">
	{% block css %}
		<link rel="stylesheet" type="text/css" href="{{ asset('css/style.css') }}">
		<link rel="stylesheet" type="text/css" href="{{ asset('css/back.css') }}">

	{% endblock %}

	{% block specific_css %}{% endblock %}

	{% block js %}
		<script src="{{ asset('js/jquery-1.11.1.js') }}" type="text/javascript"></script>
	{% endblock %}

	{% block specific_js %}{% endblock %}
    

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>

{% block header %}
<header>
	<div class="content">
		<div class="logo">
			<img src="{{ asset('img/logo.png') }}" alt="iKNSA">
			<a href="#"></a>
		</div>

		{% block user_nav %}{% endblock %}
	</div>
</header>
<div class="clear"></div>

{% endblock %} {# end block header #}



{% block nav %}{% endblock %}


{% block body %}{% endblock %} {# end block body #}

{% block footer %}
    
<footer>
</footer>

{% endblock %} {# end block footer #}
</body>
</html>